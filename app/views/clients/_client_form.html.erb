<%= render partial: "shared/error_form", locals: {new_obj: @client} %>

<%= form_for @client do |f| %>
    <%= f.label :name %>
    <div><%= f.text_field :name %></div>
    <%= f.label :username %>
    <div><%= f.text_field :username %></div>
    <%= f.label :password %>
    <div><%= f.password_field :password %></div>
    <%= f.label :phone_number %>
    <div><%= f.text_field :phone_number %></div>
    <%= f.label :email %>
    <div><%= f.text_field :email %></div>
    <%= f.label :home_address %>
    <div><%= f.text_field :home_address %></div>
    <%= f.label :isp %>
    <div><%= f.collection_select :isp_id, Isp.all, :id, :name %></div>
    <%= f.submit %>
<% end %>